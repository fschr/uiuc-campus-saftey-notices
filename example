            {
                document.write("#wt #uofi #ws-pa #edu-il h3.gadget-header {");
                document.write("color:" + setColor(skinAppGadgetsHeadingTitleColor));
                document.write("}");
            }
            
            if(applyColorFromSkinDesigner == 'false')
            {
                document.write("#wt #uofi #ws-pa #edu-il #blog-title a, #wt #uofi #ws-pa #edu-il #blog-title a:hover, #wt #uofi #ws-pa #edu-il div.title {");
                document.write("color:" + setColor(skinAppHeaderTitleLine1Color));
                document.write("}");
            }
            
            if(applyColorFromSkinDesigner == 'false')
            {
                document.write("#wt #uofi #ws-pa #edu-il .blog-description {");
                document.write("color:" + setColor(skinAppHeaderTitleLine2Color));
                document.write("}");
            }
            
            document.write("#wt #uofi #ws-pa #edu-il .blog-post-comments-section {");
            document.write("background-color:" + setColor(skinBlogPostCommentBackgroundcolor));
            document.write("}");
            
            
            if (typeof(isHeaderImage) != 'undefined' && isHeaderImage != null){
                if(isHeaderImage == 'false' && appHeaderBannerFileName.length > 0)
                {
                    document.write("#container {");
                    document.write("max-width:100%;");
                    document.write("}");
                }
            }            
            
            document.write("</style>");
            
            function setColor(values)
            {
                if(values != '')
                {
                    if(values.indexOf(",") == -1){
                        if(values == 'transparent' || values == 'TRANSPARENT'){
                            return values;
                        }
                        else{
                            return "#" + values;
                        }
                    }
                    var tempValues = values.split(",");  
                    var skinColor = tempValues[0];
                    var multiplier = tempValues[1];
                    if(skinColor == 'color1')
                    {
                        var colorR = convert(multiplier, hexToR(skinColor1));   
                        var colorG = convert(multiplier, hexToG(skinColor1)); 
                        var colorB = convert(multiplier, hexToB(skinColor1));  
                    }
                    else
                    {
                        var colorR = convert(multiplier, hexToR(skinColor1));   
                        var colorG = convert(multiplier, hexToG(skinColor1)); 
                        var colorB = convert(multiplier, hexToB(skinColor1));  
                    }
                    return rgbToHex(colorR, colorG, colorB);
                }
            }
            function hexToR(h) {return parseInt((cutHex(h)).substring(0,2),16)}
            function hexToG(h) {return parseInt((cutHex(h)).substring(2,4),16)}
            function hexToB(h) {return parseInt((cutHex(h)).substring(4,6),16)}
            function cutHex(h) {return (h.charAt(0)=="#") ? h.substring(1,7):h}
            function convert(mulitplier, color){
            var x1 = 255 - color;
            var x2 = x1 * mulitplier;
            var xC = color + x2;
            return Math.round(xC);
            }
            function rgbToHex(r, g, b) {
            return "#" + ((1 << 24) + (r << 16) + (g << 8) + b).toString(16).slice(1);
            }
